{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup",
    "predev": "npm run build:icons",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "start": "remix-serve build",
    "build": "npm run build:css && npm run build:icons && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:icons": "node scripts/svg-symbols.mjs",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "@remix-run/react": "v1.0.6",
    "@remix-run/serve": "v1.0.6",
    "cuid": "^2.1.8",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "lodash.debounce": "^4.0.8",
    "mailgun-js": "^0.22.0",
    "prisma": "^3.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remix": "v1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "v1.0.6",
    "@types/mailgun-js": "^0.22.12",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.4",
    "concurrently": "^6.4.0",
    "svgo": "2.8.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "prettier": {}
}
